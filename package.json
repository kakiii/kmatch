  "devDependencies": {
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "jest": "^29.0.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "rimraf": "^5.0.0"
