  "dependencies": {
    "@octokit/rest": "^20.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "fast-csv": "^5.0.2",
    "fast-levenshtein": "^3.0.0",
    "semver": "^7.6.0"
